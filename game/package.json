{
  "version": "0.18.1",
  "name": "hathora-et-labora-game",
  "type": "module",
  "sideEffects": false,
  "description": "Plays Uwe Rosenberg's Ora et Labora for the Hathora engine. It reduces a list of moves into a board game state.",
  "keywords": [
    "boardgame",
    "hathora",
    "ora-et-labora",
    "uwe-rosenberg"
  ],
  "homepage": "https://ora.kennerspiel.com",
  "license": "GPL-3.0",
  "author": {
    "name": "Philihp Busby",
    "email": "philihp@gmail.com",
    "url": "https://philihp.com"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier . --write",
    "lint": "eslint .",
    "test": "jest",
    "prepack": "npm run build",
    "prepare": "cd .. && husky game/.husky"
  },
  "main": "./dist/index.js",
  "files": [
    "/dist/",
    "/src/",
    "!/src/**/__tests__/*.test.ts"
  ],
  "dependencies": {
    "fast-shuffle": "6.1.1",
    "fn-pcg": "2.0.1",
    "ramda": "0.30.1",
    "ts-pattern": "5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@philihp/prettier-config": "1.0.0",
    "@tsconfig/recommended": "1.0.8",
    "@types/jest": "29.5.14",
    "@types/ramda": "0.30.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.8.3",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0"
  },
  "prettier": "@philihp/prettier-config"
}
